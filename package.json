{
    "name": "@twotwoba/vv-cli",
    "version": "2.6.1",
    "description": "Easily create Vite + React/Vue3 project with TailwindCSS and other useful libraries. Also support Chrome extension.",
    "type": "module",
    "bin": {
        "vv": "./bin/index.js"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "bin",
        "template-react",
        "template-vue",
        "template-vue-mobile",
        "template-extension",
        "!**/node_modules/**"
    ],
    "keywords": [
        "vite",
        "cli",
        "template",
        "react",
        "tailwindcss",
        "zustand",
        "swr",
        "motion",
        "vue3",
        "unocss",
        "pinia",
        "naive-ui",
        "chrome",
        "extension"
    ],
    "author": "eric.yuan",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/twotwoba/vv-cli"
    },
    "dependencies": {
        "chalk": "^5.4.1",
        "commander": "^11.1.0",
        "fs-extra": "^11.2.0",
        "inquirer": "^9.3.7",
        "ora": "^8.1.1"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/inquirer": "^9.0.7",
        "@types/node": "^22.10.1",
        "typescript": "^5.7.2"
    }
}
